{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended"
  ],
  "compilerOptions": {
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules/helmet",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/helmet"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false
    ],
    "no-empty-interface": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "semicolon": true,
    "member-access": [
      true,
      "no-public"
    ],
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}