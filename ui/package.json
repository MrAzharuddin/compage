{
  "name": "compage-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-pretty": "^2.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start": "concurrently \"cd ../ && npx ts-node app/server.ts \" \"react-scripts start\" ",
    "build": "react-scripts build && mv ./build ../build/ui",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-controls": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-storyshots": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.3.0",
    "dagre": "^0.8.5",
    "diagram-maker": "^2.0.1",
    "diagram-maker-plugin-minimap": "^1.0.1",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.3",
    "prop-types": "^15.8.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.1.2",
    "redux-undo-redo": "^2.1.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0"
  }
}
